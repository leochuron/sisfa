<%- if @item.errors.any? %>
  <%- @item.errors.full_messages.each do |msg|%>
    alertify.error("<%=j msg %>");
  <%- end %>
<%- else %>
  $(".notas").prepend("<%= j render('item_nota_enfermeras/nota', item: @item) %>");
  $('.form-notas').trigger("reset");
  alertify.success("Nota Almacenada");
<%- end %>
