<%= nested_form_for @user do |f| %>
  <%= f.text_field :username, :placeholder => "Nombre de Usuario", class: "form-control" %>
  <%= f.text_field :password, :placeholder => "Contrasena", class: "form-control" %>
  <%= f.text_field :password_confirmation, :placeholder => "Repetir contrasena", class: "form-control" %>
 <%= f.fields_for :cliente do |cliente_params| %>
 <%=  cliente_params.text_field :nombre  %>
 <%=  cliente_params.text_field :direccion  %>
 <%=  cliente_params.text_field :numero_de_identificacion  %>
 <%=  cliente_params.text_field :telefono  %>
 <%=  cliente_params.text_field :email  %>
 <% end %>
<% end %>
