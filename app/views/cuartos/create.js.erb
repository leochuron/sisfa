<%- if @cuarto.errors.any? %>
  <%- @cuarto.errors.full_messages.each do |msg|%>
    alertify.error("<%=j msg %>");
  <%- end %>
<%- else %>
  $(".cuartos").append("<%= j render('cuartos/cuarto', cuarto: @cuarto) %>");
  $('.form-cuarto').trigger("reset");
  alertify.success("Almacenado");
<%- end %>
